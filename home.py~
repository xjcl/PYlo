class Home(object):
    def __init__(self):
        self.terrain = [0, 1, 2, 3, 4, 5, 6]
        self.climate = [0, 1, 2, 3]
        self.foodchain = 1
        self.scale = 1

    def get_terrain(self):
        return self.terrain
        
    def get_climate(self):
        return self.climate

    def get_scale(self):
        return self.scale
        
    def get_foodchain(self):
        return self.foodchain
