# handles phylomon
import event_dic

class Event(object): # contrary to its name, it is an event card
    def __init__(self, pname):
        self.dic = phylo_dic.get(pname) # self.dic curr only has one element in it # TODO
        
        self.index = self.dic
    
    
    
    def get_scale(self):
        return self.scale
        
    def get_name(self):
        return self.common # include latin name?
        
    def set_scale(self, pnum):
        self.scale = pnum
        
    def get_dic(self):
        return self.dic
